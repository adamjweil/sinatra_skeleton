<%= erb :'partials/navbar' %>
<center><h1>NoteShare</h1></center>
<h3>Login</h3>
<% if @errors %>
  <% @errors.each do |error| %>
  <%= error %>
  <% end %>
<% end %>
<div id="login">
<form id="login" method="post" action="/login">
  <div>
    <!-- <br> -->
    <input type="text" name="user_info[username]"placeholder="Username"/><br>
    <!-- <input type="email" name="user_info[email]" placeholder="Email"/><br> -->
    <input type="password" name="user_info[password]" placeholder="Password"/><br>
    <input type="submit" value="submit"/>
  </div>
  <span>If you don't have an account, please register below</span>
  <a href="/register">Register</a>
</form>
</div>
