<%= erb :'partials/navbar' %>
<h1>Register</h1>
<% if @errors %>
  <% @errors.each do |error| %>
      <ul> <%= error %> </ul>
  <% end %>
<% end %>
<div id="login">
<form id="login" method="post" action="/register">
  <div>
    <!-- <br> -->
    <input type="text" name="user_info[first_name]"placeholder="First Name"/><br>
    <input type="text" name="user_info[last_name]"placeholder="Last Name"/><br>
    <input type="text" name="user_info[username]"placeholder="Username"/><br>
    <input type="email" name="user_info[email]" placeholder="Email"/><br>
    <input type="password" name="user_info[password_digest]" placeholder="Password"/><br>
    <input type="submit" value="Submit"/>
  </div>
  <span>Login Here</span>
  <a href="/index">Login</a>
</form>
</div>
